{
  "expo": {
    "name": "SEEOS",
    "slug": "seeos",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "ios": {
      "bundleIdentifier": "com.endritzhuri.seeos",
      "supportsTablet": false
    },
    "android": {
      "package": "com.endrit.seeos"
    },
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "backgroundColor": "#ffffff",
      "ldpi": "./assets/images/splash.png",
      "mdpi": "./assets/images/splash.png",
      "hdpi": "./assets/images/splash.png",
      "xhdpi": "./assets/images/splash.png",
      "xxhdpi": "./assets/images/splash.png",
      "xxxhdpi": "./assets/images/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["*/"]
  }
}